#include <stdio.h>
#include <stdlib.h>

int main(void)
{
	int num1;
	int num2;
	int resultado;
	printf("\nIngresa un numero ");
	scanf("%d",&num1);
	printf("\nIngresa un numero ");
	scanf("%d",&num2);
	resultado = num1 + num2;
	printf("El valor es: %d",resultado);
	return 0;
}
