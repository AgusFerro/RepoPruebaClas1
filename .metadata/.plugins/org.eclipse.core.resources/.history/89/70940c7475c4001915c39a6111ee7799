#include <stdio.h>
#include <stdlib.h>

int main(void) {
	int num1;
	int num2;
	int resultado;
	char texto[18];
	scanf("Ingrese un numero:","%d",&texto,&num1);
	scanf("%d",&num2);
	resultado = num1 + num2;
	printf("El valor es: %d",resultado);
	/*return EXIT_SUCCESS;*/
}
